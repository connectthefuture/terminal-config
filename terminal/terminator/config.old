[global_config]
  handle_size = 1
  title_font = Source Code Pro Light 13
  title_hide_sizetext = True
  title_inactive_bg_color = "#303030"
  title_receive_bg_color = "#303030"
  title_receive_fg_color = "#dddddd"
  title_transmit_bg_color = "#303030"
  title_transmit_fg_color = "#dddddd"
  title_use_system_font = False
[keybindings]
  cycle_next = None
  cycle_prev = None
  group_all = None
  group_all_toggle = <Super>s
  next_tab = <Primary>Tab
  prev_tab = <Primary><Shift>Tab
  split_horiz = <Primary><Shift>h
  split_vert = <Primary><Shift>d
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
[profiles]
  [[default]]
    background_color = "#181818"
    background_darkness = 0.92
    background_image = None
    cursor_color = "#f6f7ec"
    foreground_color = "#c4c5b5"
    palette = "#141414:#f3005f:#97e023:#fa8419:#1c83ff:#9d65ff:#57d1ea:#f1f1ec:#615e4b:#f4005f:#98e024:#fa8419:#1c83ff:#9d65ff:#58d1eb:#f1f1ec"
    scrollback_infinite = True
    scrollbar_position = hidden
